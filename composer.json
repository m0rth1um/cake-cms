{
    "name": "scherersoftware/cake-cms",
    "description": "Block-based Content Management System for CakePHP 3",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "Cms"
    },
    "require": {
        "cakephp/plugin-installer": "*",
        "codekanzlei/cake-cktools": "2.*",
        "codekanzlei/cake-frontend-bridge": "v2.0.0-rc3 as 1.4.11",
        "codekanzlei/cake-attachments": "*",
        "codekanzlei/cake-model-history": "*"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0"
    },
    "autoload": {
        "psr-4": {
        "Cms\\": "src"
        }
    }
}
